{
  "name": "ran-boilerplate",
  "version": "0.6.1",
  "main": "server.js",
  "description": "Free Code Camp Voting App Challenge",
  "author": "Eddie Naff <eddie@eddienaff.com> (http://eddienaff.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sly777/ran"
  },
  "bugs": {
    "url": "https://github.com/Sly777/ran/issues"
  },
  "keywords": [
    "next.js",
    "react",
    "graphql",
    "boilerplate",
    "apollo",
    "styled-components",
    "ran",
    "run"
  ],
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=production node ./node_modules/.bin/pm2 start server.js -i max --attach",
    "build": "npm run clean-cache && next build",
    "clean-cache": "rm -rf ./node_modules/.cache",
    "dev": "npm run lint && npm run clean-cache && node server.js",
    "dev:next": "npm run lint && npm run clean-cache && next",
    "start:next": "node ./node_modules/.bin/pm2 start npm --name \"next\" -- start",
    "lint": "eslint --fix --ext .js pages components containers libraries server helper_scripts",
    "lint:watch": "esw -w --fix components libraries server",
    "analyze": "ANALYZE_BUILD=true npm run build",
    "setup": "node ./helper_scripts/CL_commands/setup.js && npm run setup:after",
    "setup:after": "npm run build",
    "create:page": "node ./helper_scripts/CL_commands/create_page.js",
    "create:route": "node ./helper_scripts/CL_commands/create_route.js",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "heroku-postbuild": "npm run build",
    "release": "release",
    "test": "jest --watchAll --runInBand",
    "cover": "jest --coverage --no-cache",
    "graphql:play": "graphql playground",
    "graphql:update_schema": "graphql get-schema",
    "graphql:see_graph": "graphql voyager"
  },
  "dependencies": {
    "apollo-server-express": "^1.1.3",
    "babel-plugin-inline-import-graphql-ast": "2.0.1",
    "babel-plugin-styled-components": "1.2.1",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chalk": "2.1.0",
    "color": "2.0.0",
    "compression": "1.7.1",
    "connect-redis": "^3.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "2.8.4",
    "d3": "^4.11.0",
    "dataloader": "^1.3.0",
    "dotenv": "4.0.0",
    "express": "4.16.1",
    "express-bearer-token": "^2.1.0",
    "graphql": "^0.10.5",
    "graphql-iso-date": "^3.3.0",
    "graphql-tools": "^2.2.1",
    "helmet": "3.8.2",
    "i": "^0.3.6",
    "ip": "1.1.5",
    "is_js": "^0.9.0",
    "isomorphic-fetch": "2.2.1",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "js-cookie": "2.1.4",
    "jsonwebtoken": "^8.0.1",
    "lodash": "^4.17.4",
    "lru-cache": "4.1.1",
    "moment": "2.18.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.12.0",
    "next": "3.2.2",
    "next-cookies": "1.0.2",
    "next-routes": "1.0.40",
    "nodemon": "^1.12.1",
    "offline-plugin": "4.8.4",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^3.0.0",
    "pm2": "2.7.1",
    "prop-types": "15.6.0",
    "react": "15.6.2",
    "react-apollo": "1.4.16",
    "react-dom": "15.6.2",
    "react-faux-dom": "^4.0.5",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "styled-components": "2.2.1",
    "uuid": "^3.1.0",
    "webpack-bundle-analyzer": "2.9.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.1",
    "cli-clear": "1.0.4",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-graphql": "1.4.0-1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.2",
    "figlet": "1.2.0",
    "graphql-cli": "1.1.0",
    "graphql-cli-voyager": "0.1.2",
    "handlebars": "4.0.10",
    "husky": "0.14.3",
    "inquirer": "3.3.0",
    "lint-staged": "4.2.3",
    "ngrok": "2.2.22",
    "pre-commit": "1.2.2",
    "prettier": "1.7.4",
    "release": "2.5.2",
    "replace-in-file": "3.0.0-beta.2",
    "shelljs": "0.7.8"
  }
}
